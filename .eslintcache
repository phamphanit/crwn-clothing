ribefromAuth();\n  }\n  render() {\n    return (\n      <div>\n        <Header />\n        <Switch>\n          <Route exact path='/' component={HomePage} />\n          <Route exact path='/shop' component={ShopPage} />\n          <Route exact path='/signin'\n            render={() => this.props.currentUser ? (<Redirect to='/' />) : (<SignInAndSignUpPage />)}\n          />\n        </Switch>\n\n      </div>\n    );\n  }\n}\nconst mapStateToProps = ({ user }) =>\n({\n  currentUser: user.currentUser\n\n})\nconst mapDispatchToProps = dispatch => (\n  {\n    setCurrentUser: user => dispatch(setCurrentUser(user))\n  }\n)\nexport default connect(mapStateToProps, mapDispatchToProps)(App);\n","/Users/phamngocphan/Desktop/crwn-clothing/src/index.js",[],"/Users/phamngocphan/Desktop/crwn-clothing/src/pages/homepage/homepage.component.jsx",[],"/Users/phamngocphan/Desktop/crwn-clothing/src/components/directory/directory.component.jsx",[],"/Users/phamngocphan/Desktop/crwn-clothing/src/components/menu-item/menu-item.component.jsx",[],"/Users/phamngocphan/Desktop/crwn-clothing/src/pages/shop/shop.component.jsx",[],"/Users/phamngocphan/Desktop/crwn-clothing/src/pages/shop/shop.data.js",[],"/Users/phamngocphan/Desktop/crwn-clothing/src/components/preview-collection-component/collection-preview.component.jsx",[],"/Users/phamngocphan/Desktop/crwn-clothing/src/components/collection-item/collection-item.component.jsx",["124"],"import React from \"react\";\nimport \"./collection-item.styles.scss\";\nimport { connect } from \"react-redux\";\nimport { addItem } from \"../../redux/cart/cart.actions\";\nimport CustomButton from \"../custom-button/custom-button.component\";\nconst CollectionItem = ({ item, addItem }) => {\n  const { id, name, price, imageUrl } = item;\n  return (\n    <div className=\"collection-item\">\n      <div\n        className=\"image\"\n        style={{ backgroundImage: `url(${imageUrl})` }}\n      ></div>\n      <div className=\"collection-footer\">\n        <span class